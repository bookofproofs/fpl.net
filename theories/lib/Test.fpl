uses Fpl.Commons

def cl A: obj {intr}
def cl B: A {intr}
def cl C: obj {ctor C() {dec base.obj (); self }}
def cl D: B {ctor D() {dec base.B(); self }}

def pred T() 
{ 
    dec ~o:object 
    ~a:A 
    ~aI1:A aI1:=A 
    ~aI2:A aI2:=A() 
    ~b:B 
    ~bI1:B bI1:=B 
    ~bI2:B bI2:=B() 
    ~c:C 
    ~cI1:C cI1:=C 
    ~cI2:C cI2:=C() 
    ~d:D 
    ~dI1:D dI1:=D 
    ~dI2:D dI2:=D() 
    ;

    (a = b = c = d = o)
}
;